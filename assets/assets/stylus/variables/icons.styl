@font-face
  font-family "icons"
  src url("~@/static/fonts/icons.woff") format("woff"), url("~@/static/fonts/icons.svg") format("svg")
  font-weight normal
  font-style normal
  font-display block

iconFont()
  font-family "icons" !important
  speak none
  font-style normal
  font-weight normal
  line-height 1
  font-variant normal
  text-transform none
  -webkit-font-smoothing antialiased
  -moz-osx-font-smoothing grayscale

